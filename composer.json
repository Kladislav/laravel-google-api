{
    "name": "kladislav/laravel-google-api",
    "description": "Google API Provider for Laravel projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Kim",
            "email": "kim@mediacube.network"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "league/oauth2-google": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kladislav\\LaravelGoogleApi\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}
